// Configuration settings file format documentation can be found at:
// https://help.apple.com/xcode/#/dev745c5c974

// Development Team
// Set this to your Apple Developer Team ID
// You can find this in Xcode under Signing & Capabilities
// or in the Apple Developer portal
DEVELOPMENT_TEAM = 

// Code Signing
// Leave empty for automatic signing
CODE_SIGN_IDENTITY = 
CODE_SIGN_STYLE = Automatic

// Bundle Identifier
PRODUCT_BUNDLE_IDENTIFIER = com.jaces.mac

// API Configuration
// These are compile-time constants for default values
// Users can override these in the app settings
DEFAULT_SERVER_URL = http:/$()/localhost:3000
DEFAULT_DEVICE_TOKEN_PREFIX = dev_tk_

// Sync Settings
DEFAULT_SYNC_INTERVAL = 60
DEFAULT_BATCH_SIZE = 100
DEFAULT_MAX_RETRIES = 5